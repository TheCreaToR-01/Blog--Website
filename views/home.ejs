<!-- Header Include -->
<%- include("partials/header") -%>

    <h1>Home</h1>
    <p>
        <%= homeContent %>
    </p>

    <% blog.forEach(function(blog){ %>
        <h1>
            <%= blog.title %>
        </h1>
        <p>
            <%= blog.content.substring(0, 100) + " ..." %>
            <a href="/blogs/<%= blog.title %>">Read More</a>
        </p>
        <% }) %>

            <!-- Footer Include -->
            <%- include("partials/footer") -%>